import java.util.*;

public class Main {
  public static void printElement(int a,int b){
    System.out.printf("%d + %d = %d\t", a, b, a + b );
  }
  public static void printRow(int n){
    for (int i=1; i<=9; i++){
      printElement (n,i);
    }
    System.out.println();
  }
      public static void printTable(){
        for (int i = 1; i<=9; i++){
          printRow (i);

    }
  }
    public static void main(String[] args) {
printTable();  
    }
}

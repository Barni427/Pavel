import java.util.*;

public class Main {
  public static int findFibRecursion(int n){
    if ((n == 1) || (n == 2))
    return 1;
    else
    return findFibRecursion (n-1) + findFibRecursion(n-2);
    
    
  }
    public static void main(String[] args) {
      
      System.out.println(findFibRecursion(20));
  }
}
